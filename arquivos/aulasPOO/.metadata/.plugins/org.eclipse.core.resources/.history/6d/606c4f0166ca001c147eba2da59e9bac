package org.serratec.persistence;

import org.serratec.model.Funcionario;

public class ExemplosStream {

	public static void main(String[] args) {
		Funcionario a1 = new Funcionario(1, "Roni", "pedro@gmail.com", 28);
		Funcionario a2 = new Funcionario(2, "Pedro", "pedro2@gmail.com", 29);
		Funcionario a3 = new Funcionario(3, "Fábio", "pedro3@gmail.com", 30);
		Funcionario a4 = new Funcionario(4, "Fabiano", "pedro4@gmail.com", 31);
		Funcionario a5 = new Funcionario(5, "Francarlo", "pedro5@gmail.com", 32);
		
	}

}
