package org.serratec.persistence;

import org.serratec.model.Funcionario;

public class ExemplosStream {

	public static void main(String[] args) {
		Funcionario a1 = new Funcionario(1, "Roni", "RH", 1500.00);
		Funcionario a2 = new Funcionario(2, "Pedro", "DP", 4000.00);
		Funcionario a3 = new Funcionario(3, "Fábio", "PO", 5000.00);
		Funcionario a4 = new Funcionario(4, "Fabiano", "CPD", 3000.00);
		Funcionario a5 = new Funcionario(5, "Francarlo", "AI", 2300.00);
		
	}

}
